      <nombre>fecha</nombre>
      <tipo>date</tipo>
      <nulo>NO</nulo>
   </columna>
   <columna>
      <nombre>version</nombre>
      <tipo>character varying(15)</tipo>
      <nulo>NO</nulo>
   </columna>
   <columna>
      <nombre>descargas</nombre>
      <tipo>integer</tipo>
   </columna>
   <columna>
      <nombre>activos</nombre>
      <tipo>integer</tipo>
   </columna>
   <restriccion>
      <nombre>comm3_stats_pkey</nombre>
      <consulta>PRIMARY KEY (fecha,version)</consulta>
   </restriccion>
</tabla>